{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route":"{route:alpha}/{id:int?}",
      "methods": ["get", "post", "put", "delete"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "direction": "in",
      "name": "teamsfxContext",
      "type": "TeamsFx"
    }
  ]
}
